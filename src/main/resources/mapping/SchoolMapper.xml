<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stucar.dao.SchoolDao">
    <resultMap id="school" type="School">
        <id column="shl_id" property="shl_id" />
        <result column="shl_name" property="shl_name" />
        <result column="shl_address" property="shl_address" />
    </resultMap>
    <select id="selectAllSchool" resultType="school">
       select * from `school`
    </select>
</mapper>